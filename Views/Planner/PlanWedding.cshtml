@model planner.Models.PlanViewModel
<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Index</title>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <link rel="stylesheet" href="~/css/style.css"/>
    </head>
    <body>
        <div class="wrapper">
            <h1>Write a Review</h1>
            <a href="/logout">Log Out</a>
            <a href="/dashboard">Dashboard</a><br>
            <form asp-controller="Planner" asp-action="NewWedding" method="post">
                <label asp-for="Wedder1">Wedder 1:</label>
                <input asp-for="Wedder1"/><br>
                <label asp-for="Wedder2">Wedder 2:</label>
                <input asp-for="Wedder2"/><br>
                <label asp-for="Date">Date:</label>
                <input asp-for="Date"/><br>
                <label asp-for="Address">Wedding Address:</label>
                <input asp-for="Address"/><br>
                <button type="submit">Create</button>
            </form>
            <br>
            <div asp-validation-summary="All"></div>
        </div>
    </body>
</html>